<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup Label="Configuration">
    <SignAssembly>True</SignAssembly>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>net46;netstandard1.6;net45;net461</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>sign.snk</AssemblyOriginatorKeyFile>
    <OutputType>library</OutputType>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
    <EmbeddedResource Include="**\*.resx" />
    <None Include="sign.snk" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="9.0.*" />
    <PackageReference Include="Serilog" Version="2.4.*" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)'=='netstandard1.6'">
    <PackageReference Include="NETStandard.Library" Version="1.6" />
    <PackageReference Include="System.Net.NameResolution" Version="4.3.*" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)'=='net45|net46|net461'">
    <Reference Include="System" />
  </ItemGroup>
</Project>